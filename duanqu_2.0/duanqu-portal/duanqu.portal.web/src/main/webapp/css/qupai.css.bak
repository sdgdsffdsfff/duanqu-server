body {
	margin:0;
	padding:0;
	font:12px/150% Tahoma, Geneva, sans-serif;
	background:#ddd;
	color:444;
}
.clearfix:before, .clearfix:after {
	content: ".";
	display: table;
	font-size: 0;
	height: 0;
	line-height: 0;
	visibility: hidden;
}
.clearfix:after {
	clear: both;
}
.r {
	float:right
}
img {
	display:block;
	border:none
}
.content {
	width:980px;
	margin:auto
}
#Logo {
	background:url(../images/logo.png) no-repeat;
	width:217px;
	height:104px;
}
#Logo a {
	display:block;
	width:100%;
	height:100%;
}
#Phone {
}
.iphone {
	background:url(../images/iphone.png) no-repeat;
	width:344px;
	height:646px;
	margin-top:15px;
	float:left;
	padding:126px 22px 0 39px;
	margin-left:-600px;
	opacity:0.1
}
.iphone ul {
	padding:0;
	margin:0
}
.iphone li {
	list-style:none;
}
.focus {
	width:306px;
	height:457px;
	overflow:hidden;
	position:relative;
}
.focus ul {
	height:457px;
	position:absolute;
}
.focus ul li {
	float:left;
	width:306px;
	height:457px;
	overflow:hidden;
	position:relative;
}
.focus ul li div {
	position:absolute;
	overflow:hidden;
}
.focus .btnBg {
	position:absolute;
	width:306px;
	height:20px;
	left:0;
	bottom:0;
	background:#000;
	display:none;
}
.focus .btn {
	position:absolute;
	height:23px;
	padding:0px 10px ;

	bottom:10px;
	width:306px;
	text-align:center;
}
.focus .btn span {
	display:inline-block;
	_display:inline;
	_zoom:1;
	width:10px;
	height:10px;
	line-height:0;
	text-indent:-99999px;
	border-radius:5px;
	margin-left:15px;
	cursor:pointer;
	background:#fff;
}
.focus .btn span.on {
	background:#fff;
}
.focus .preNext {
	width:45px;
	height:457px;
	position:absolute;
	top:0;
	cursor:pointer;
}
.focus .pre {
	left:0;
}
.focus .next {
	right:0;
	background-position:right top;
}
.text {
	float:right;
	width:500px;
	padding:50px 0 0 35px;
	margin-right:-500px
}
.featherbg {
	background:url(../images/gridbg.jpg);
	border-top:10px solid #fff;
	padding:40px 0 80px 0;
	margin:0 auto
}
.featherbg ul {
	padding:0;
	margin:0
}
.featherbg li {
	list-style:none;
	float:left;
	width:236px;
	height:294px;
	margin-right:12px;
	overflow:hidden
}
.featherbg li div {
	width:234px;
	height:189px;
	padding-top:25px;
	text-align:center
}
.featherbg li p {
	padding:10px;
	font-family:"微软雅黑";
	color:#525252;
	font-size:16px;
	line-height:130%;
	background:#fff;
	margin:0;
	height:80px;
}
.featherbg li.n0 div {
	background:#f04929 url(../images/f1.png) center center no-repeat;
}
.featherbg li div img {
	margin-left:35px
}
.featherbg li.n1 div {
	background:#7daf54 url(../images/f2.png) center center no-repeat;
}
.featherbg li.n2 div {
	background:#ffb427 url(../images/f3.png) center center no-repeat;
}
.featherbg li.n3 div {
	background:#00c7c4 url(../images/f4.png) center center no-repeat;
}
.featherbg li:hover img {
	-webkit-animation:transform-5 1s ease 0s alternate none infinite;
	-moz-animation:transform-5 1s ease 0s alternate none infinite;
	animation:transform-5 1s ease 0s alternate none infinite;
}
 @-webkit-keyframes transform-5 {
from {
-webkit-transform:perspective(400px) rotateY(0deg);
}
to {
	-webkit-transform:perspective(400px) rotateY(360deg);
}
}
@-moz-keyframes transform-5 {
from {
-moz-transform:perspective(400px) rotateY(0deg);
}
to {
	-moz-transform:perspective(400px) rotateY(360deg);
}
}
@keyframes transform-5 {
from {
transform:perspective(400px) rotateY(0deg);
}
to {
	transform:perspective(400px) rotateY(360deg);
}
}
